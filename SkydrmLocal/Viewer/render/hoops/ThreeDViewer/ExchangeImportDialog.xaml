<Window x:Class="Viewer.render.hoops.ThreeDViewer.ExchangeImportDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Viewer.render.hoops.ThreeDViewer"
        mc:Ignorable="d"
        Title="Loading..." 
        Height="116" 
        Width="311"
        ResizeMode="NoResize"
        ShowInTaskbar="False"
        WindowStartupLocation="CenterOwner">
    <Grid>
        
        <StackPanel HorizontalAlignment="Center" Width="305" Orientation="Vertical">
            <!--<ProgressBar x:Name="progress_bar" Minimum="0" Maximum="100" Margin="10,10,11,10" Height="20" Width="284"/>-->
            <ProgressBar x:Name="progress_bar" Height="23" Margin="10" Width="270"
                         VerticalAlignment="Top"  
                         IsIndeterminate="True"/>
            
            <Button  x:Name="CancelButton" Click="CancelButton_Click" IsCancel="True" Width="109" 
                     Content="Cancel" HorizontalAlignment="Center" Margin="0,4,0,0"/>
        </StackPanel>
        
    </Grid>
</Window>
