<Page x:Class="Viewer.upgrade.ui.normal.page.toolbar.view.ToolBarPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Viewer.upgrade.ui.normal.page.toolbar.view"
      mc:Ignorable="d" 
      Loaded="Page_Loaded"
      Unloaded="Page_Unloaded">

    <Grid>
        <!--File name-->
        <TextBlock FontWeight="Bold" TextTrimming="CharacterEllipsis" HorizontalAlignment="Left"
                       VerticalAlignment="Center" Margin="10,0,0,0" MaxWidth="450" Text="{Binding FileName}" />
    </Grid>
</Page>
