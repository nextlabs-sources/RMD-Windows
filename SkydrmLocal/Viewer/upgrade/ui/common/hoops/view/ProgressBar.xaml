<Window x:Class="Viewer.upgrade.ui.common.hoops.view.ProgressBar"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d"
        Title="Loading..." 
        Height="116" 
        Width="311"
        ResizeMode="NoResize"
        ShowInTaskbar="False"
        WindowStartupLocation="CenterOwner"
        Loaded="Window_Loaded">
    <Grid>
        <StackPanel HorizontalAlignment="Center" Width="305">
            <ProgressBar x:Name="progress_bar" Minimum="0" Maximum="100" Margin="10,10,11,10" Height="20" Width="284"/>
            <Button  x:Name="CancelButton" Click="CancelButton_Click" IsCancel="True" Width="109" Content="Cancel" Margin="98,0"/>
        </StackPanel>
    </Grid>
</Window>
