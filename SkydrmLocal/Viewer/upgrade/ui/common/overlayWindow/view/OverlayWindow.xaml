<Window x:Class="Viewer.upgrade.ui.common.overlayWindow.view.OverlayWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Viewer.upgrade.ui.common.overlayWindow.view"
        mc:Ignorable="d"
        AllowsTransparency = "True"
        WindowStyle="None"
        ShowInTaskbar="False"        
        Focusable="True"
        Loaded="Window_Loaded" 
        ContentRendered="Window_ContentRendered"
        MouseWheel="Window_MouseWheel" 
        MouseLeftButtonDown="Window_MouseLeftButtonDown"
        MouseLeftButtonUp="Window_MouseLeftButtonUp"
        >
    
    <Window.Background>
        <SolidColorBrush Opacity="0.01" Color="White"></SolidColorBrush>
    </Window.Background>

    <Grid Name="Host_Grid" Background="Transparent">

    </Grid>
</Window>
