<Window x:Class="SkydrmLocal.rmc.ui.windows.ShareWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SkydrmLocal"
        xmlns:UserControl="clr-namespace:SkydrmLocal.rmc.ui.components"
        mc:Ignorable="d"
        ResizeMode="NoResize"
        Closing="Window_Closing"
        Closed="WIindow_Closed"
        KeyUp="Window_KeyUp"
        Title="SkyDRM DESKTOP" Height="600" Width="1000" WindowStartupLocation="CenterScreen" SizeToContent="Height">
    <Grid>
        <Frame Name="main_frame" NavigationUIVisibility="Hidden"/>
        
        <!--For progress ui display -->
        <Grid x:Name="GridProBar" Height="600" Background="LightGray" Opacity="0.5" Visibility="Collapsed">
            <!--For loading progress background-->
            <UserControl:ProBarComponent />
        </Grid>
        
    </Grid>
   
</Window>
